# 認証関連
AUTH_SECRET=your_auth_secret_here # 認証用の秘密鍵（ランダムな文字列を設定）
AUTH_URL=https://example.com/api/auth # 認証サービスのURL（本番環境では実際のURLに変更）

# Google OAuth認証
GOOGLE_ID=your_google_client_id # Google Cloud ConsoleでOAuth 2.0クライアントIDを取得
GOOGLE_SECRET=your_google_client_secret # Google Cloud ConsoleでOAuth 2.0クライアントシークレットを取得

# Cloudflare D1データベース
# 注: Cloudflare Workersの場合、DB環境変数は自動的にバインドされるため、
# ローカル開発時のみ必要です

# Stripe決済
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key # Stripeダッシュボードから取得
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret # Stripeダッシュボードで設定したWebhookシークレット

# WebRTC (SkyWay)
SKYWAY_APP_ID=your_skyway_app_id # SkyWayダッシュボードから取得
SKYWAY_SECRET=your_skyway_secret # SkyWayダッシュボードから取得
